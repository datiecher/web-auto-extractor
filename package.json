{
  "name": "@rane/web-auto-extractor",
  "version": "2.0.7",
  "description": "Automatically extracts structured information from webpages",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "mocha --recursive --require babel-core/register",
    "lint": "standard",
    "dev": "nodemon --exec npm run lint",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "^0.22.0",
    "html-entities": "^2.4.0",
    "htmlparser2": "^9.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "sinon": "^15.2.0",
    "standard": "^17.1.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "tonicExample.js",
      "dist/"
    ]
  },
  "keywords": [
    "crawler",
    "parser",
    "html parser",
    "schema.org",
    "microdata",
    "rdfa",
    "jsonld",
    "seo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/raine/web-auto-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/raine/web-auto-extractor/issues"
  },
  "tonicExampleFilename": "tonicExample.js"
}
